{
  "parser": "babel-eslint",
  "env" : {
    "es6" : true,
    "browser" : true,
    "node" : true
  },
  "rules" : {
    "semi": [2, "never"],
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "no-debugger": "off",
    "linebreak-style": 0
    // "no-console": "off",
  },
  "extends": ["eslint:recommended", "google"]
}